    "tailwindcss": "^3.4.19"
  },
  "devDependencies": {
    "@types/react": "~18.3.12",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "typescript": "~5.6.2"
